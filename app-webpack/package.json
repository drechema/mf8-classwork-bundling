{
  "name": "app-webpack",
  "version": "1.0.0",
  "description": "Example app bundle with webpack",
  "repository": "https://github.com/drechema/mf8-classwork-bundling",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.development.js",
    "build:dev": "rimraf dist && webpack --config webpack.development.js",
    "build:prod": "rimraf dist && webpack --config webpack.production.js",
    "build:size": "rimraf dist && webpack --config webpack.analyzer.js"
  },
  "keywords": [],
  "author": "David Reche",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
